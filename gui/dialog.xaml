<Window x:Class="gui.Dialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gui"
        mc:Ignorable="d"
        Title="アダプター選択" Height="135.99" Width="250.473" ResizeMode="NoResize" Icon="nC.ico" WindowStyle="ToolWindow" Loaded="Window_Loaded">
    <Grid>
        <Label Content="対象ネットワークアダプターを選択してください。" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0"/>
        <Button x:Name="button1" Content="決定" HorizontalAlignment="Left" Margin="10,68,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" IsEnabled="False"/>
        <ComboBox x:Name="combo1" HorizontalAlignment="Left" Margin="10,41,0,0" Width="120" SelectionChanged="ComboBox_SelectionChanged" Height="22" VerticalAlignment="Top"/>
        <Button Content="再読込" Margin="135,41,0,0" VerticalAlignment="Top" Click="Button_Click_1" Height="22" HorizontalAlignment="Left" Width="75"/>
        <Button Content="終了" HorizontalAlignment="Left" Margin="135,68,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>

    </Grid>
</Window>
